    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run vitest",
            "runtimeExecutable": "vitest",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "args": []
        },
        {
            "name": "Debug parser",
            "type": "debugpy",
            "request": "launch",
            "program": "src/python_support/parse.py",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        }
    ]
}